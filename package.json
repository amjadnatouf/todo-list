{
  "homepage": "http://amjadnatouf.github.io/todos",
  "name": "todos",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "author": "amjad-natouf",
  "scripts": {
    "dev": "vite",
    "predeploy": "vite build",
    "deploy": "gh-pages-deploy -- 'A custom commit message'",
